    "class point:\n",
    "    \n",
    "    def __init__(self, x,y,z):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "        self.z=z\n",
    "        \n",
    "    def sqsum(self):\n",
    "        return (self.x*self.x+self.y*self.y+self.z*self.z)\n",
    "        \n",
    "x1=point(1,3,5)\n",
    "print(x1.sqsum())"
   ]
  }
